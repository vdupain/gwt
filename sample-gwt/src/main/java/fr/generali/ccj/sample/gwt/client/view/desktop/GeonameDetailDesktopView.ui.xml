<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:sample='urn:import:fr.generali.ccj.sample.gwt.client.view.desktop'>

  <ui:style>
    .detail {
    	border: 1px solid #666;
    	background-color: white;
    }
    
  </ui:style>

  <g:DockLayoutPanel unit='EM' styleName='{style.detail}'>
    <g:center>
      <g:AbsolutePanel>
          <g:Grid>
            <g:row>
              <g:customCell><g:Label text="name" /></g:customCell>
              <g:customCell><g:TextBox ui:field="name"/></g:customCell>
              <g:customCell><g:Label text="asciiname" /></g:customCell>
              <g:customCell><g:TextBox  ui:field="asciiname" /></g:customCell>
              <g:customCell><g:Label text="countryCode" /></g:customCell>
              <g:customCell><g:TextBox ui:field="countryCode"/></g:customCell>
            </g:row>
            <g:row>
              <g:customCell><g:Label text="alternatenames" /></g:customCell>
              <g:customCell><g:TextArea ui:field="alternatenames"/></g:customCell>
            </g:row>
            <g:row>
              <g:customCell><g:Label text="longitude" /></g:customCell>
              <g:customCell><g:TextBox ui:field="longitude"/></g:customCell>
              <g:customCell><g:Label text="latitude" /></g:customCell>
              <g:customCell><g:TextBox ui:field="latitude"/></g:customCell>
            </g:row>
          </g:Grid>
      </g:AbsolutePanel>
    </g:center>
  </g:DockLayoutPanel>

</ui:UiBinder>
