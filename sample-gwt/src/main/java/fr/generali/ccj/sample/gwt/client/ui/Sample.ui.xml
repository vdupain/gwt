<ui:UiBinder
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:sample='urn:import:fr.generali.ccj.sample.gwt.client.ui'>

  <g:DockLayoutPanel unit='EM'>
    <g:north size='5'>
      <sample:TopPanel ui:field='topPanel' />
    </g:north>

    <g:center>
      <g:SplitLayoutPanel>
        <g:west size='192'>
          <sample:Shortcuts ui:field='shortcuts' />
        </g:west>

        <g:north size='200'>
          <sample:CountryList ui:field='countryList' />
        </g:north>

        <g:center>
          <sample:CountryDetail ui:field='countryDetail' />
        </g:center>
      </g:SplitLayoutPanel>
    </g:center>
  </g:DockLayoutPanel>

</ui:UiBinder>
